{
  "parser": "babel-eslint",
  "extends": "airbnb",
   "plugins": [
    "react",
    "react-native"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "global-require": 0
  },
  "globals": {
    "$": true,
    "document": true,
    "window": true,
    "localStorage": true,
    "require": true
  }
}